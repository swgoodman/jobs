<h1>Sign In!</h1>
<p>Don't have an account? <%= link_to "Sign Up", new_user_path %></p>

  <%= form_for @user, :url => url_for(:controller => 'sessions', :action => 'create') do |f| %>
    <%= f.label :email %>

    <%= f.text_field :email %>
    <br>
    <%= f.label :password %>

    <%= f.password_field :password %>
    <br>
    <%= f.submit "Sign In" %>
  <% end %>
